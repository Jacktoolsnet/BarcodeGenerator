<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19"
      fx:controller="net.jacktools.barcode.barcodegenerator.MainViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <ImageView fx:id="imageViewBarcode" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"/>
    <Button onAction="#onHelloButtonClick" text="%button.generate.barcode"/>
</VBox>
