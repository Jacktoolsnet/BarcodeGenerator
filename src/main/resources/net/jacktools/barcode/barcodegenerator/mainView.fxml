<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19"
            fx:controller="net.jacktools.barcode.barcodegenerator.MainViewController">
    <top>
        <ButtonBar buttonMinWidth="40.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button fx:id="buttonSave" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="48.0"
                        maxWidth="48.0" minHeight="48.0" minWidth="48.0" mnemonicParsing="false"
                        onAction="#buttonSave_onAction" prefHeight="48.0" prefWidth="48.0" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../../img/floppy-disk.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="%button.savebarcode.tooltop"/>
                    </tooltip>
                </Button>
                <Button fx:id="buttonCopy" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="48.0"
                        maxWidth="48.0" minHeight="48.0" minWidth="48.0" mnemonicParsing="false"
                        onAction="#buttonCopy_onAction" prefHeight="48.0" prefWidth="48.0" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../../img/copy.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="%button.copytoclipboard.tooltop"/>
                    </tooltip>
                </Button>
                <Button fx:id="buttonHelp" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="48.0"
                        maxWidth="48.0" minHeight="48.0" minWidth="48.0" mnemonicParsing="false"
                        onAction="#buttonHelp_onAction" prefHeight="48.0" prefWidth="48.0" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../../img/circle-question.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="Empty Tooltip"/>
                    </tooltip>
                </Button>
                <Button fx:id="buttonInfo" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" layoutX="830.0"
                        layoutY="10.0" maxHeight="48.0" maxWidth="48.0" minHeight="48.0" minWidth="48.0"
                        mnemonicParsing="false" onAction="#buttonInfo_onAction" prefHeight="48.0" prefWidth="48.0"
                        textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../../img/circle-info.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="Empty Tooltip"/>
                    </tooltip>
                </Button>
                <Button fx:id="buttonCloseApplication" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" layoutX="915.0"
                        layoutY="10.0" maxHeight="48.0" maxWidth="48.0" minHeight="48.0" minWidth="48.0"
                        mnemonicParsing="false" onAction="#buttonCloseApplication_onAction" prefHeight="48.0"
                        prefWidth="48.0" textAlignment="CENTER">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../../../img/door-open.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="%button.closeApplication.tooltop"/>
                    </tooltip>
                </Button>
            </buttons>
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </ButtonBar>
    </top>
    <center>
        <TabPane fx:id="tabPaneMain" side="LEFT" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="40.0" tabMaxWidth="40.0"
                 tabMinHeight="40.0" tabMinWidth="40.0" BorderPane.alignment="CENTER">
            <tabs>
                <Tab fx:id="tabBarcode" closable="false">
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" rotate="90.0">
                            <image>
                                <Image url="@../../../../img/barcode.png"/>
                            </image>
                            <rotationAxis>
                                <Point3D/>
                            </rotationAxis>
                        </ImageView>
                    </graphic>
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane layoutX="98.0" layoutY="36.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <top>
                                        <ButtonBar buttonMinWidth="40.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <Label alignment="CENTER_RIGHT" layoutX="218.0" layoutY="17.0"
                                                       maxWidth="40.0" text="%fxml.main.label.type"/>
                                                <ChoiceBox fx:id="choiceBoxBarcodeType" maxWidth="100.0"
                                                           minWidth="100.0" prefWidth="100.0"/>
                                                <Label alignment="CENTER_RIGHT" maxWidth="40.0"
                                                       text="%fxml.main.label.value"/>
                                                <TextField fx:id="textFieldBarcodeValue"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="420.0" layoutY="22.0"
                                                       maxWidth="40.0" text="%fxml.main.label.width"/>
                                                <Spinner fx:id="spinnerBarcodeWidth" maxWidth="75.0" minWidth="75.0"
                                                         prefWidth="75.0"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="469.0" layoutY="26.0"
                                                       maxWidth="40.0" text="%fxml.main.label.height"/>
                                                <Spinner fx:id="spinnerBarcodeHeight" layoutX="554.0" layoutY="22.0"
                                                         maxWidth="75.0" minWidth="75.0" prefWidth="75.0"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="578.0" layoutY="16.0"
                                                       maxWidth="100.0" text="%fxml.main.label.barcodecolor"/>
                                                <ColorPicker fx:id="colorPickerBarcode" maxWidth="100.0"
                                                             minWidth="100.0" prefWidth="100.0">
                                                    <value>
                                                        <Color/>
                                                    </value>
                                                </ColorPicker>
                                                <Label alignment="CENTER_RIGHT" layoutX="716.0" layoutY="16.0"
                                                       maxWidth="100.0" text="%fxml.main.label.backgroundcolor"/>
                                                <ColorPicker fx:id="colorPickerBarcodeBackground" layoutX="801.0"
                                                             layoutY="12.0" maxWidth="100.0" minWidth="100.0"
                                                             prefWidth="100.0"/>
                                            </buttons>
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </ButtonBar>
                                    </top>
                                    <center>
                                        <ImageView fx:id="imageViewBarcode" pickOnBounds="true" preserveRatio="true"
                                                   smooth="false" BorderPane.alignment="CENTER">
                                            <image>
                                                <Image url="@../../../../img/barcode.png"/>
                                            </image>
                                            <viewport>
                                                <Rectangle2D/>
                                            </viewport>
                                        </ImageView>
                                    </center>
                                    <left>
                                        <TableView fx:id="tableViewBarcode" editable="true"
                                                   BorderPane.alignment="CENTER">
                                            <columns>
                                                <TableColumn fx:id="tableColumnBarcodeDesignation" editable="false"
                                                             maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
                                                             text="%tableColumnBarcodeDesignation"/>
                                                <TableColumn fx:id="tableColumnBarcodeValue" maxWidth="150.0"
                                                             minWidth="150.0" prefWidth="150.0"
                                                             text="%tableColumnBarcodeValue"/>
                                            </columns>
                                            <BorderPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </BorderPane.margin>
                                        </TableView>
                                    </left>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="tabQrCode" closable="false">
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <top>
                                        <ButtonBar buttonMinWidth="40.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <Label alignment="CENTER_RIGHT" layoutX="420.0" layoutY="22.0"
                                                       maxWidth="40.0" text="%fxml.main.label.width"/>
                                                <Spinner fx:id="spinnerQrCodeWidth" maxWidth="75.0" minWidth="75.0"
                                                         prefWidth="75.0"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="469.0" layoutY="26.0"
                                                       maxWidth="40.0" text="%fxml.main.label.height"/>
                                                <Spinner fx:id="spinnerQrCodeHeight" layoutX="554.0" layoutY="22.0"
                                                         maxWidth="75.0" minWidth="75.0" prefWidth="75.0"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="707.0" layoutY="14.0"
                                                       maxWidth="100.0" text="%fxml.main.label.barcodecolor"/>
                                                <ColorPicker fx:id="colorPickerQrCode">
                                                    <value>
                                                        <Color/>
                                                    </value>
                                                </ColorPicker>
                                                <Label alignment="CENTER_RIGHT" layoutX="792.0" layoutY="14.0"
                                                       maxWidth="100.0" text="%fxml.main.label.backgroundcolor"/>
                                                <ColorPicker fx:id="colorPickerQrCodeBackground" layoutX="792.0"
                                                             layoutY="10.0">
                                                    <value>
                                                        <Color/>
                                                    </value>
                                                </ColorPicker>
                                            </buttons>
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </ButtonBar>
                                    </top>
                                    <center>
                                        <ImageView fx:id="imageViewQrCode" pickOnBounds="true" preserveRatio="true"
                                                   BorderPane.alignment="CENTER">
                                            <image>
                                                <Image url="@../../../../img/qrcode.png"/>
                                            </image>
                                        </ImageView>
                                    </center>
                                    <left>
                                        <Accordion fx:id="accordionQrCodes" BorderPane.alignment="CENTER">
                                            <panes>
                                                <TitledPane fx:id="titledPaneTransfer" animated="false"
                                                            text="%fxml.main.titledpane.transfer">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0">
                                                            <children>
                                                                <VBox layoutX="80.0" layoutY="14.0" spacing="5.0"
                                                                      AnchorPane.bottomAnchor="0.0"
                                                                      AnchorPane.leftAnchor="0.0"
                                                                      AnchorPane.rightAnchor="0.0"
                                                                      AnchorPane.topAnchor="0.0">
                                                                    <children>
                                                                        <Label text="%epc.label.bic"/>
                                                                        <TextField fx:id="textFieldBic"/>
                                                                        <Label layoutX="15.0" layoutY="15.0"
                                                                               text="%epc.label.payee"/>
                                                                        <TextField fx:id="textFieldPayee" layoutX="15.0"
                                                                                   layoutY="37.0"/>
                                                                        <Label layoutX="15.0" layoutY="68.0"
                                                                               text="%epc.label.iban"/>
                                                                        <TextField fx:id="textFieldIban" layoutX="15.0"
                                                                                   layoutY="90.0"/>
                                                                        <Label layoutX="15.0" layoutY="120.0"
                                                                               text="%epc.label.paymentAmount"/>
                                                                        <Spinner fx:id="spinnerPaymentAmount"/>
                                                                        <Label layoutX="15.0" layoutY="120.0"
                                                                               text="%epc.label.purpose"/>
                                                                        <TextField fx:id="textFieldPurpose"
                                                                                   layoutX="15.0" layoutY="143.0"/>
                                                                        <Label layoutX="15.0" layoutY="173.0"
                                                                               text="%epc.label.reference"/>
                                                                        <TextField fx:id="textFieldReference"
                                                                                   layoutX="15.0" layoutY="196.0"/>
                                                                        <Label layoutX="15.0" layoutY="226.0"
                                                                               text="%epc.label.purposeOfUse"/>
                                                                        <TextField fx:id="textFieldPurposeOfUse"
                                                                                   layoutX="15.0" layoutY="301.0"/>
                                                                        <Label layoutX="15.0" layoutY="332.0"
                                                                               text="%epc.label.notice"/>
                                                                        <TextField fx:id="textFieldNotice"
                                                                                   layoutX="15.0" layoutY="354.0"/>
                                                                    </children>
                                                                    <padding>
                                                                        <Insets bottom="5.0" left="5.0" right="5.0"
                                                                                top="5.0"/>
                                                                    </padding>
                                                                </VBox>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                                <TitledPane fx:id="titledContact" animated="false"
                                                            text="%fxml.main.titledpane.contact">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </TitledPane>
                                                <TitledPane fx:id="titledPaneMeeting" animated="false"
                                                            text="%fxml.main.titledpane.meeting">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                                    prefWidth="200.0"/>
                                                    </content>
                                                </TitledPane>
                                                <TitledPane fx:id="titledPaneHyoerlink" animated="false" layoutX="10.0"
                                                            layoutY="60.0" text="%fxml.main.titledpane.hyperlink">
                                                    <content>
                                                        <AnchorPane>
                                                            <children>
                                                                <VBox layoutX="58.0" layoutY="61.0"
                                                                      AnchorPane.bottomAnchor="0.0"
                                                                      AnchorPane.leftAnchor="0.0"
                                                                      AnchorPane.rightAnchor="0.0"
                                                                      AnchorPane.topAnchor="0.0">
                                                                    <children>
                                                                        <Label text="%hyperlink.label.hyperlink"/>
                                                                        <TextField fx:id="textfieldHyperlink"/>
                                                                    </children>
                                                                </VBox>
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </TitledPane>
                                            </panes>
                                        </Accordion>
                                    </left>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" rotate="90.0">
                            <image>
                                <Image url="@../../../../img/qrcode.png"/>
                            </image>
                            <rotationAxis>
                                <Point3D/>
                            </rotationAxis>
                        </ImageView>
                    </graphic>
                </Tab>
                <Tab fx:id="tabWebServer" closable="false">
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane layoutX="150.0" layoutY="60.0" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <top>
                                        <ButtonBar buttonMinWidth="40.0" BorderPane.alignment="CENTER">
                                            <buttons>
                                                <CheckBox fx:id="checkBoxWebServerAutoStart" mnemonicParsing="false"
                                                          text="beim Programmstart starten"/>
                                                <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" maxWidth="30.0"
                                                       minWidth="30.0" prefWidth="30.0" text="Port:"
                                                       textAlignment="RIGHT"/>
                                                <Spinner fx:id="spinnerWebServerPort" maxWidth="100.0" minWidth="100.0"
                                                         prefWidth="100.0"/>
                                                <Button fx:id="buttonWebServerStart" alignment="CENTER"
                                                        contentDisplay="GRAPHIC_ONLY" maxHeight="48.0" maxWidth="48.0"
                                                        minHeight="48.0" minWidth="48.0" mnemonicParsing="false"
                                                        onAction="#buttonWebServerStart_onAction" prefHeight="48.0"
                                                        prefWidth="48.0" textAlignment="CENTER">
                                                    <graphic>
                                                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true"
                                                                   preserveRatio="true">
                                                            <image>
                                                                <Image url="@../../../../img/circle-play.png"/>
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                    <padding>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </padding>
                                                    <tooltip>
                                                        <Tooltip text="%button.webserverStart.tooltop"/>
                                                    </tooltip>
                                                </Button>
                                                <Button fx:id="buttonWebServerStop" alignment="CENTER"
                                                        contentDisplay="GRAPHIC_ONLY" layoutX="496.0" layoutY="10.0"
                                                        maxHeight="48.0" maxWidth="48.0" minHeight="48.0"
                                                        minWidth="48.0" mnemonicParsing="false"
                                                        onAction="#buttonWebServerStop_onAction" prefHeight="48.0"
                                                        prefWidth="48.0" textAlignment="CENTER">
                                                    <graphic>
                                                        <ImageView fitHeight="36.0" fitWidth="36.0" pickOnBounds="true"
                                                                   preserveRatio="true">
                                                            <image>
                                                                <Image url="@../../../../img/circle-stop.png"/>
                                                            </image>
                                                        </ImageView>
                                                    </graphic>
                                                    <padding>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </padding>
                                                    <tooltip>
                                                        <Tooltip text="%button.webserverStop.tooltop"/>
                                                    </tooltip>
                                                </Button>
                                            </buttons>
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </padding>
                                        </ButtonBar>
                                    </top>
                                    <center>
                                        <TextArea fx:id="textAreaWebServerLog" editable="false" prefHeight="200.0"
                                                  prefWidth="200.0" BorderPane.alignment="CENTER"/>
                                    </center>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                    <graphic>
                        <ImageView fitHeight="36.0" fitWidth="36.0" rotate="90.0">
                            <image>
                                <Image url="@../../../../img/rss.png"/>
                            </image>
                            <rotationAxis>
                                <Point3D/>
                            </rotationAxis>
                        </ImageView>
                    </graphic>
                </Tab>
            </tabs>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </TabPane>
    </center>
    <bottom>
        <Hyperlink fx:id="hyperlinkPreview" onAction="#hyperlinkPreview_onAction" text="Hyperlink"
                   BorderPane.alignment="CENTER"/>
    </bottom>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</BorderPane>
